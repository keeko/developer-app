{
	"name" : "keeko/developer-app",
	"description" : "App with developer information and API docs.",
	"type" : "keeko-app",
	"license" : "MIT",
	"keywords" : [
		"keeko",
		"api",
		"apidoc",
		"dev",
		"developer"
	],
	"authors" : [{
			"name" : "gossi",
			"email" : "github@gos.si"
		}
	],
	"autoload" : {
		"psr-4" : {
			"keeko\\application\\developer\\" : "src"
		}
	},
	"require" : {
		"php" : ">=5.4",
		"keeko/composer-installer" : "*",
		"puli/repository" : "^1.0@beta",
		"fxp/composer-asset-plugin" : "~1.0",
		"bower-asset/bootstrap" : "~3",
		"bower-asset/font-awesome" : "~4",
		"bower-asset/swagger-ui" : "~2"
	},
	"require-dev" : {
		"keeko/core" : "dev-master",
		"composer/composer" : "@dev",
		"propel/propel" : "@dev",
		"puli/composer-plugin" : "@beta",
		"puli/twig-extension" : "@beta",
		"puli/url-generator" : "@beta",
		"puli/discovery" : "@beta"
	},
	"extra" : {
		"keeko" : {
			"app" : {
				"title" : "Developer Application",
				"class" : "\\keeko\\application\\developer\\DeveloperApplication"
			}
		}
	}
}